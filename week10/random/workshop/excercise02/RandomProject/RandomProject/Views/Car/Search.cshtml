@model System.Collections.Generic.List<RandomProject.Models.Car>
<!DOCTYPE html>
<html>
<head>
    <title>Car Search</title>
    <meta charset="utf-8" />
</head>
<body>
    <div>
        <div>
            <form action="/search" method="get">
                <input type="text" name="q" value="" />
                <button type="submit" value="Search">Search</button>
            </form>
        </div>
        <a href="/search?diplomat=1">Diplomats only</a>
        <a href="/search?police=1">Police only</a>
    </div>
    <div>
        <table style="width:100%; text-align: center" border="1">
            <tr>
                <th>Plate</th>
                <th>Model</th>
                <th>Brand</th>
                <th>Color</th>
                <th>Year</th>
            </tr>
            @foreach (var element in Model)
            {
                <tr>
                    <td>@element.Plate</td>
                    <td>@element.CarModel</td>
                    <td><a href="/search/@(element.CarBrand)">@element.CarBrand</a></td>
                    <td>@element.Color</td>
                    <td>@element.Year</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>